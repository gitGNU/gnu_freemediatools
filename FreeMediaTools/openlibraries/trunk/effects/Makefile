install-nobase_dist_openeffectslib_dataDATA: $(nobase_dist_openeffectslib_data_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(openeffectslib_datadir)" || $(mkdir_p) "$(DESTDIR)$(openeffectslib_datadir)"
	@$(am__vpath_adj_setup) \
	list='$(nobase_dist_openeffectslib_data_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  $(am__vpath_adj) \
	  echo " $(nobase_dist_openeffectslib_dataDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(openeffectslib_datadir)/$$f'"; \
	  $(nobase_dist_openeffectslib_dataDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(openeffectslib_datadir)/$$f"; \
	done

uninstall-nobase_dist_openeffectslib_dataDATA:
	@$(NORMAL_UNINSTALL)
	@$(am__vpath_adj_setup) \
	list='$(nobase_dist_openeffectslib_data_DATA)'; for p in $$list; do \
	  $(am__vpath_adj) \
	  echo " rm -f '$(DESTDIR)$(openeffectslib_datadir)/$$f'"; \
	  rm -f "$(DESTDIR)$(openeffectslib_datadir)/$$f"; \
	done
