MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-rtripple

first: all

all: Makefile $(SUBTARGETS)

rtripple/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "rtripple" || $(MKDIR) "rtripple"
	cd rtripple && $(QMAKE) rtripple.pro -o $(MAKEFILE)
sub-rtripple: rtripple/$(MAKEFILE) FORCE
	cd rtripple && $(MAKE) -f $(MAKEFILE)

Makefile: rtplugins.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile rtplugins.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile rtplugins.pro

all: $(SUBTARGETS)
qmake_all: rtripple/$(MAKEFILE)
	 ( [ -d rtripple ] && cd rtripple ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d rtripple ] && cd rtripple ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d rtripple ] && cd rtripple ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d rtripple ] && cd rtripple ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d rtripple ] && cd rtripple ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

